  <object class="GtkListStore" id="album_liststore">
    <columns>
      <!-- column-name label -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Alle Alben</col>
      </row>
    </data>
  </object>
  <object class="GtkListStore" id="artist_liststore">
    <columns>
      <!-- column-name label -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Alle Künstler</col>
      </row>
    </data>
  </object>
  <object class="GtkListStore" id="genre_liststore">
